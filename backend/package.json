{
  "name": "financeinfo-pro-backend",
  "version": "1.0.0",
  "description": "Backend API para FinanceInfo Pro - SaaS de gest√£o financeira para infoprodutores",
  "main": "dist/server.js",
  "scripts": {
    "build": "npx prisma generate && tsc --project tsconfig.build.json",
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "dev:watch": "nodemon --exec ts-node src/server.ts",
    "postinstall": "npx prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "ts-node prisma/seed.ts",
    "seed:expenses": "ts-node scripts/seed-expenses.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "lint": "echo \"Linting not configured yet\"",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "saas",
    "fintech",
    "infoprodutor",
    "financial-management",
    "node.js",
    "typescript"
  ],
  "author": "FinanceInfo Pro Team",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.0.4",
    "bcryptjs": "^3.0.2",
    "bull": "^4.16.5",
    "cors": "^2.8.5",
    "dotenv": "^16.6.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.10",
    "prisma": "^6.10.1",
    "redis": "^5.5.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "zod": "^3.25.67",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0"
  }
}
